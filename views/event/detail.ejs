<h2><%= event.name %></h2>

<div>
    Description: <%= event.description  %>
</div>

<div>
    Start Date: <%= event.startDate  %>
</div>

<div>
    End Date: <%= event.endDate  %>
</div>

<div>
    Time: <%= event.time  %>
</div>

<div>
    Image: <%= event.image  %>
</div>

<div>
    Location: <%= event.location  %>
</div>

<div>
    Category: <%= event.category.map(c => c.name).join(', ')%>
</div>